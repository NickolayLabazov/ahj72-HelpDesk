{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "class-methods-use-this": "off",
        "no-restricted-syntax": "off",
        "no-empty": "off",
        "no-useless-constructor": "off",
        "max-len": "off",
        "import/no-cycle": "off"

    }
}